{
  "extends": "@fenixui-org/tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "paths": {
    "@/*": [
      "./*"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}